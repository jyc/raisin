(compile -s -O3 -d1 async.scm -j async)
(compile -s -O3 -d0 async.import.scm)

(compile -s -O3 -d1 async-extras.scm -j async-extras)
(compile -s -O3 -d0 async-extras.import.scm)

(install-extension
  'async
  '("async.so" "async.import.so")
  '((version "0.1"))
)

(install-extension
  'async-extras
  '("async-extras.so" "async-extras.import.so")
  '((version "0.1"))
)
